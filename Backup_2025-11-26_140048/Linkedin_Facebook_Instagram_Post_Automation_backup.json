      "parameters": {
        "jsCode": "return items.map(item => {\n  const data = item.json;\n  \n  // Get the content and trim it\n  let content = (data[\"Linkedin Post Content\"] || \"\").trim();\n  \n  // Convert escaped newlines back to actual newlines\n  content = content.replace(/\\\\n/g, '\\n');\n  \n  // Also handle any other escaped characters that might be present\n  content = content.replace(/\\\\\"/g, '\"');\n  \n  return {\n    json: {\n      linkedinContent: content\n    }\n  };\n});"
      },
