      "id": "ea2d7ecd-b9df-4809-8e06-c398ea1f935f",
      "name": "3. Find CEO LinkedIn Profile",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4,
      "position": [
        1952,
        0
      ],
      "credentials": {
        "httpHeaderAuth": {
          "id": "odw0FaTRUSGiHCOl",
