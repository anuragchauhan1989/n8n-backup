    {
      "parameters": {
        "jsCode": "// Get the transcript data from the previous node\nconst transcriptData = $input.first().json;\n\n// Initialize variable to store clean transcript\nlet cleanTranscript = '';\n\n// Check if transcript exists and is an array\nif (transcriptData.transcript && Array.isArray(transcriptData.transcript)) {\n  \n  // Extract only the text from each segment and join them\n  cleanTranscript = transcriptData.transcript\n    .map(segment => {\n      // Decode HTML entities (like &#39; to ')\n      let text = segment.text || '';\n      \n      // Replace common HTML entities\n      text = text\n        .replace(/&#39;/g, \"'\")\n        .replace(/&quot;/g, '\"')\n        .replace(/&amp;/g, '&')\n        .replace(/&lt;/g, '<')\n        .replace(/&gt;/g, '>')\n        .replace(/&nbsp;/g, ' ');\n      \n      return text;\n    })\n    .join(' ')  // Join all text segments with a space\n    .trim();    // Remove leading/trailing whitespace\n    \n} else if (typeof transcriptData.transcript === 'string') {\n  // If transcript is already a string, just clean it\n  cleanTranscript = transcriptData.transcript\n    .replace(/&#39;/g, \"'\")\n    .replace(/&quot;/g, '\"')\n    .replace(/&amp;/g, '&')\n    .replace(/&lt;/g, '<')\n    .replace(/&gt;/g, '>')\n    .replace(/&nbsp;/g, ' ')\n    .trim();\n}\n\n// Return the formatted data with clean transcript\nreturn [{\n  json: {\n    ...($json),  // Keep all existing fields\n    'Transcript': cleanTranscript,  // Add/update the clean transcript\n    transcriptLength: cleanTranscript.length,  // Optional: track length\n    transcriptWordCount: cleanTranscript.split(' ').length  // Optional: track word count\n  }\n}];\n\n// Log for debugging\nconsole.log('Transcript extracted successfully');\nconsole.log('Character count:', cleanTranscript.length);\nconsole.log('Word count:', cleanTranscript.split(' ').length);"
      },
